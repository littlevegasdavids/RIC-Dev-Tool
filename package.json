{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "concurrently \"npx tailwindcss -i ./src/input.css -o ./public/global.css\" \"rollup -c\" ",
    "dev": "concurrently \"npx tailwindcss -i ./src/input.css -o ./public/global.css --watch\" \"rollup -c -w\"",
    "start": "node server.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "daisyui": "^2.46.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.9.0",
    "sirv-cli": "^2.0.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "svelte-routing": "^1.6.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
